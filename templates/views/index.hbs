{{!< default}}
<div class="container">
	<div class="row">
			
		<div class="col s12 m6">
			<div class="card grey lighten-2 z-depth-3">
				<div class="card-image">
				<img class="responsive-img" src="http://placehold.it/400x500">
				</div>
				<button class="btn-flat left blue lighten-1 "><- </button>   <button class="btn right blue lighten-1">-> </button>
			</div>
		</div>	
			
		<div class="col s12 m6 ">
			<div class="card-panel blue lighten-2 info-right z-depth-3">
		
		
		
			<table class="bordered centered">
				<tbody>
					{{# each stats}}
						
						<tr> 
							<td> {{name}} </td> <td> {{value}}   </td>
						
						</tr>
					{{/each}}
				</tbody>
				
			</table>
			
			
			</div>
		</div>		
			
				
	</div>
	 
	<div class="row">
		<div class="col s12 m6">
			
			<img class="responsive-img" src="http://placehold.it/400x600">
			
		</div>	
	</div>

</div>

